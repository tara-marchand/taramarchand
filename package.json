{
  "name": "tmarchand",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app/index.tsx",
  "engines": {
    "node": "^12"
  },
  "type": "module",
  "scripts": {
    "build:app": "babel ./app/src -d ./app/dist --config-file=./app/.babelrc",
    "build:dev": "BABEL_ENV=development NODE_ENV=development PORT=3000 npm run clean; webpack --config=webpack.dev.js --colors",
    "build:prod": "BABEL_ENV=production npm run clean; webpack --config=webpack.prod.js --env.NODE_ENV=production --display=errors-only --colors",
    "build:dev:stats": "webpack --config=webpack.dev.js --env.NODE_ENV=development --json > stats-dev.json",
    "build:prod:stats": "webpack --config=webpack.prod.js --env.NODE_ENV=production --json > stats-prod.json",
    "clean": "rm -rf ./static/dist",
    "postinstall": "./postinstall.sh",
    "start:prod": "npm run build:app && DEBUG=express:* node ./app/dist/index.js",
    "start:dev": "npm run build:app && DEBUG=express:* node ./app/dist/index.js; npm run build:dev",
    "start": "npm run start:dev",
    "stats": "npm run build:dev:stats; npm run build:prod:stats",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "plugins": [
      [
        "lodash",
        {
          "id": [
            "lodash"
          ]
        }
      ]
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      }
    }
  },
