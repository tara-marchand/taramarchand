{
  "name": "tmarchand",
  "version": "1.0.0",
  "description": "",
  "main": "app/index.js",
  "engines": {
    "node": "^8",
    "yarn": "^1"
  },
  "scripts": {
    "build:dev": "yarn run clean; webpack --mode=development --config=webpack.dev.js --env.NODE_ENV=development --env.PORT=3000 --display=errors-only --colors",
    "build:prod": "yarn run clean; webpack --mode=production --config=webpack.prod.js --env.NODE_ENV=production --display=errors-only --colors",
    "build:dev:stats": "webpack --mode=development --config=webpack.dev.js --env.NODE_ENV=development --json > stats-dev.json",
    "build:prod:stats": "webpack --mode=production --config=webpack.prod.js --env.NODE_ENV=production --json > stats-prod.json",
    "clean": "rm -rf static/dist",
    "dev": "yarn run build:dev; DEBUG=express:* babel-node --presets=env,react --plugins=react-hot-loader/babel app/index.js",
    "start": "yarn run build:prod; babel-node --presets=env,react app/index.js",
    "stats": "yarn run build:dev:stats; yarn run build:prod:stats",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "plugins": [
      "react-hot-loader/babel",
      [
        "lodash",
        {
          "id": [
            "lodash",
            "semantic-ui-react"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.24.1",
    "cloudinary": "^1.11.0",
    "compression": "^1.7.1",
    "css-loader": "^0.26.4",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.5",
    "express": "^4.15.2",
    "express-handlebars": "^3.0.0",
    "file-loader": "^1.1.11",
    "global": "^4.3.2",
    "image-webpack-loader": "^3.2.0",
    "japanese-kana": "^0.1.2",
    "jsdom": "^11.8.0",
    "mime-types": "^2.1.17",
    "mini-css-extract-plugin": "^0.4.0",
    "morgan": "^1.9.0",
    "newrelic": "^4.0.0",
    "node-fetch": "^2.1.2",
    "node-sass": "^4.5.0",
    "postcss-loader": "^2.1.5",
    "precss": "^1.4.0",
    "prop-types": "^15.6.1",
    "react": "^16",
    "react-dom": "^16",
    "react-hot-loader": "^4.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.3",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.2",
    "style-loader": "^0.13.2",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "url-loader": "^0.5.8",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "^1.12.1",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.22.2"
  }
