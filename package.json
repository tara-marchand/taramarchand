{
  "name": "taramarchand.com",
  "version": "1.0.0",
  "description": "taramarchand.com",
  "main": "src/app/layout.tsx",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/tara-marchand/taramarchand.git"
  },
  "engines": {
    "node": "^16",
    "npm": "^9"
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "scripts": {
    "build:next": "npm run clean:next && NODE_ENV=production next build",
    "build:server": "npm run clean:server && tsc --project tsconfig.server.json",
    "clean:next": "rimraf .next",
    "clean:server": "rimraf .server",
    "start": "npm run start:dev",
    "start:dev": "npm run build:next; NODE_ENV=development npm run build:server; env-cmd -f './.env' node --trace-warnings .server/server/index.js",
    "start:prod": "npm run build:next; NODE_ENV=production npm run build:server; node .server/server/index.js",
    "start:prod:local": "npm run build:next; NODE_ENV=production npm run build:server; node .server/server/index.js",
    "lint": "next lint",
    "postbuild": "next-sitemap"
  },
  "author": "",
  "license": "ISC",
