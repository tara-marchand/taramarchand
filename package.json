  "scripts": {
    "clean": "rm -rf ./static/dist",
    "build": "npm run build:prod",
    "build:dev": "npm run clean && NODE_ENV=development npx webpack --config=webpack.dev.js --env NODE_ENV=development",
    "build:prod": "npm run clean && NODE_ENV=production npx webpack --config=webpack.prod.js --env NODE_ENV=production",
    "build:dev:stats": "npx webpack --config=webpack.dev.js --env NODE_ENV=development --json > stats/stats-dev.json",
    "build:prod:stats": "npx webpack --config=webpack.prod.js --env NODE_ENV=production --json > stats/stats-prod.json",
    "start": "npm run start:dev",
    "start:dev": "npm run build:dev; NEW_RELIC_ENABLED=false NODE_ENV=development node app/index-dev",
    "start:prod": "NEW_RELIC_ENABLED=true NODE_ENV=production node app/index-prod",
    "start:prod:local": "npm run build:prod; NEW_RELIC_ENABLED=false NODE_ENV=production node app/index-prod",
    "stats": "concurrently \"build:dev:stats\" \"npm run build:prod:stats\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier:checkJs": "prettier --parser \"babel\" --list-different \"static/src/**/*.{js,jsx}\"",
    "prettier:writeJs": "prettier --parser \"babel\" --write \"static/src/**/*.{js,jsx}\"",
    "prettier:checkTs": "prettier --parser \"typescript\" --list-different \"static/src/**/*.{ts,tsx}\"",
    "prettier:writeTs": "prettier --parser \"typescript\" --write \"static/src/**/*.{ts,tsx}\""
  },
