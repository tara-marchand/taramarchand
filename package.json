  "scripts": {
    "clean": "rm -rf ./static/dist",
    "clean:app": "rm -rf ./app/dist",
    "build:app:dev": "npm run clean:app && NEW_RELIC_ENABLED=false NODE_ENV=development PORT=4400 webpack --config=config/webpack.server.js",
    "build:app:prod": "npm run clean:app && NEW_RELIC_ENABLED=true NODE_ENV=production webpack --config=config/webpack.server.js",
    "build:dev": "npm run clean && NODE_ENV=development webpack --config=config/webpack.dev.js --env.NODE_ENV=development",
    "build": "npm run build:prod",
    "build:prod": "npm run clean && NODE_ENV=production webpack --config=config/webpack.prod.js --env.NODE_ENV=production --display-errors-only",
    "build:dev:stats": "webpack --config=config/webpack.dev.js --env.NODE_ENV=development --json > stats/stats-dev.json",
    "build:prod:stats": "webpack --config=config/webpack.prod.js --env.NODE_ENV=production --json > stats/stats-prod.json",
    "start": "npm run start:dev",
    "start:dev": "npm run build:dev; npm run build:app:dev && NEW_RELIC_ENABLED=false NODE_ENV=development PORT=4400 node app/dist",
    "start:prod": "npm run build:app:prod && NEW_RELIC_ENABLED=true NODE_ENV=production node app/dist",
    "start:prod:local": "npm run build:prod; npm run build:app:prod && NEW_RELIC_ENABLED=false NODE_ENV=production node app/dist",
    "stats": "concurrently \"build:dev:stats\" \"npm run build:prod:stats\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier:checkJs": "prettier --parser \"babel\" --list-different \"static/src/**/*.{js,jsx}\"",
    "prettier:writeJs": "prettier --parser \"babel\" --write \"static/src/**/*.{js,jsx}\"",
    "prettier:checkTs": "prettier --parser \"typescript\" --list-different \"static/src/**/*.{ts,tsx}\"",
    "prettier:writeTs": "prettier --parser \"typescript\" --write \"static/src/**/*.{ts,tsx}\""
  },
