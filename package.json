{
  "name": "tmarchand",
  "version": "1.0.0",
  "description": "",
  "main": "src/app/index.tsx",
  "engines": {
    "node": "^9",
    "yarn": "^1"
  },
  "scripts": {
    "build:app":
      "babel ./app/src/index.js --out-file=./app/dist/index.js --presets es2015 --ignore=./node_modules",
    "build:dev":
      "yarn run clean; yarn run build:app; webpack --mode=development --config=webpack.dev.js --env.NODE_ENV=development --env.PORT=3000 --display=errors-only --colors",
    "build:prod":
      "yarn run clean; yarn run build:app; webpack --mode=production --config=webpack.prod.js --env.NODE_ENV=production --display=errors-only --colors",
    "build:dev:stats":
      "webpack --mode=development --config=webpack.dev.js --env.NODE_ENV=development --json > stats-dev.json",
    "build:prod:stats":
      "webpack --mode=production --config=webpack.prod.js --env.NODE_ENV=production --json > stats-prod.json",
    "clean": "rm -rf static/dist",
    "start:prod":
      "yarn run build:prod; BABEL_ENV=production NODE_ENV=production node-esml ./app/dist/index.js",
    "start:dev":
      "yarn run build:dev; BABEL_ENV=development NODE_ENV=development DEBUG=express:* node-esml ./app/dist/index.js",
    "start": "yarn run start:dev",
    "stats": "yarn run build:dev:stats; yarn run build:prod:stats",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "plugins": [
      [
        "lodash",
        {
          "id": ["lodash"]
        }
      ]
    ],
    "env": {
      "development": {
        "plugins": ["react-hot-loader/babel"]
      }
    }
  },
  "dependencies": {
    "@blueprintjs/core": "^3.0.1",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.24.1",
    "cloudinary": "^1.11.0",
    "compression": "^1.7.1",
    "css-loader": "^1.0.0",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.5",
    "express": "^4.15.2",
    "express-handlebars": "^3.0.0",
    "file-loader": "^1.1.11",
    "global": "^4.3.2",
    "image-webpack-loader": "^3.2.0",
    "japanese-kana": "^0.1.2",
    "jsdom": "^11.8.0",
    "less": "^3.5.3",
    "less-loader": "^4.1.0",
    "mime-types": "^2.1.17",
    "mini-css-extract-plugin": "^0.4.0",
    "morgan": "^1.9.0",
    "newrelic": "^4.0.0",
    "node-es-module-loader": "^0.3.8",
    "node-fetch": "^2.1.2",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.5",
    "precss": "^1.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "url-loader": "^0.5.8",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@types/react-router": "^4.0.28",
    "@types/react-router-dom": "^4.2.7",
    "babel-eslint": "^8.2.2",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "^1.12.1",
    "react-hot-loader": "^4.3.3",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.22.2"
  }
}
