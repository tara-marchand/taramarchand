{
  "editor.formatOnSave": true,
  "files.associations": {
    "*.js": "javascriptreact",
    "*.ts": "typescriptreact"
  },
  "tslint.configFile": "tslint.json",
  "tslint.exclude": [
    "*.js",
    "*.jsx"
  ],
  "tslint.autoFixOnSave": false,
  "typescript.format.enable": false,
  "typescript.reportStyleChecksAsWarnings": false
}
