{
  "files.associations": {
    "*.js": "javascriptreact",
    "*.ts": "typescriptreact"
  },
  "typescript.format.enable": false,
  "typescript.reportStyleChecksAsWarnings": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": false,
  "jest.jestCommandLine": "node_modules/.bin/jest",
  "grammarly.selectors": [
    {
      "language": "markdown",
      "scheme": "untitled",
      "pattern": "Untitled-1"
    },
    {
      "language": "tex",
      "scheme": "untitled",
      "pattern": "Untitled-1"
    }
  ]
}