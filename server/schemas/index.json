{
  "$schema": "http://json-schema.org/schema#",
  "$id": "https://www.taramarchand.com/",
  "title": "Schema",
  "definitions": {
    "sms": {
      "$id": "#sms",
      "type": "object",
      "required": ["fromEmail", "message"],
      "properties": {
        "fromEmail": { "type": "string" },
        "message": { "type": "string" }
      }
    },
    "user": {
      "$id": "#user",
      "type": "object",
      "required": ["email"],
      "properties": {
        "email": {
          "type": "string"
        },
        "id": {
          "type": "number"
        },
        "password": {
          "type": "string"
        }
      }
    },
    "signup-reply": {
      "$id": "#signup-reply",
      "type": "object",
      "properties": {
        "token": {
          "type": "object"
        },
        "user": {
          "type": "number"
        }
      }
    }
  },
  "type": "object"
}
